{
  "name": "convex-logging-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "Monorepo for Convex logging system",
  "packageManager": "pnpm@10.5.0",
  "scripts": {
    "dev": "pnpm --parallel --filter \"./apps/*\" dev",
    "dev:ingest-server": "pnpm --filter @repo/ingest-server dev",
    "dev:dashboard": "pnpm --filter @repo/dashboard dev",
    "build": "pnpm --filter \"./apps/*\" build",
    "build:ingest-server": "pnpm --filter @repo/ingest-server build",
    "build:dashboard": "pnpm --filter @repo/dashboard build",
    "start:ingest-server": "pnpm --filter @repo/ingest-server start"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
